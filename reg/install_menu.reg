[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\shell\pdf-renamer-zimo]
@="使用 PDF重命名工具 重命名"
"Icon"="F:\\Code\\location\\A. YukunWuCode\\论文重命名\\pdfrename-gui\\dist\\PDFrename_zimo.exe"

[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\shell\pdf-renamer-zimo\command]
@="\"F:\\Code\\location\\A. YukunWuCode\\论文重命名\\pdfrename-gui\\dist\\PDFrename_zimo.exe\" \"%1\""